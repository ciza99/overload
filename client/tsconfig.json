{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "paths": {
      "@components": ["./src/components"],
      "@constants": ["./src/constants"],
      "@hooks": ["./src/hooks"],
      "@pages": ["./src/pages"],
      "@models": ["./src/models"],
      "@api": ["./src/api"],
      "@utils": ["./src/utils"],
      "@schemas": ["./src/schemas"],
      "@src": ["./src"],

      "@components/*": ["./src/components/*"],
      "@constants/*": ["./src/constants/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@pages/*": ["./src/pages/*"],
      "@models/*": ["./src/models/*"],
      "@api/*": ["./src/api/*"],
      "@utils/*": ["./src/utils/*"],
      "@schemas/*": ["./src/schemas/*"],
      "@src/*": ["./src/*"]
    }
  },
  "references": [{ "path": "../server" }],
  "extends": "expo/tsconfig.base"
}
